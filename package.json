{
  "name": "laraxios",
  "version": "1.1.1",
  "description": "Axios wrapper for Laravel API",
  "source": "./src/index.ts",
  "main": "./dist/laraxios.js",
  "module": "./dist/module.js",
  "types": "./dist/types.d.ts",
  "targets": {
    "main": {
      "isLibrary": true
    },
    "module": {
      "isLibrary": true,
      "outputFormat": "esmodule"
    }
  },
  "author": "Nenad Novkovic <shonetow@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/codetow/laraxios.git"
  },
  "bugs": {
    "url": "https://github.com/codetow/laraxios/issues"
  },
  "homepage": "https://github.com/codetow/laraxios.git",
  "keywords": [
    "axios",
    "laravel",
    "api",
    "ajax"
  ],
  "scripts": {
    "watch": "parcel watch",
    "clean": "npx rimraf dist/*",
    "lint": "eslint . --ext .ts",
    "build": "npm run clean && standard --fix && parcel build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "devDependencies": {
    "@parcel/packager-ts": "^2.8.1",
    "@parcel/transformer-typescript-types": "^2.8.1",
    "@types/jest": "^29.2.4",
    "@typescript-eslint/eslint-plugin": "^5.48.1",
    "@typescript-eslint/parser": "^5.48.1",
    "axios": "^1.2.1",
    "eslint": "^8.31.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-n": "^15.6.0",
    "eslint-plugin-promise": "^6.1.1",
    "jest": "^29.0.1",
    "jest-environment-jsdom": "^29.0.1",
    "parcel": "^2.7.0",
    "rimraf": "^3.0.2",
    "standard": "^17.0.0",
    "ts-jest": "^29.0.0-next.0",
    "typescript": "^4.9.4"
  }
}
